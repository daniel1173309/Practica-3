#include <stdio.h>

int main() {
    int num;
    printf("Ingrese un número entero positivo: ");
    scanf("%d", &num);
    
    printf("Los factores de %d son: ", num);
    for (int i = 1; i <= num; i++) {
        if (num % i == 0) {
            printf("%d ", i);
        }
            
    }
    
    if(num<=0){
        printf("Error, ingrese un número positivo");
    }
    return 0;
}
